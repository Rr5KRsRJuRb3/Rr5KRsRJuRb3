<script type="text/javascript">
function switchForm(newFormName) {
flattripForm = document.getElementById("c4p_form_flattrip");
ownarrivalForm = document.getElementById("c4p_form_ownarrival");
flightForm = document.getElementById("c4p_form_flight");

flattripSelect = document.getElementById("radioButtons-flattrip");
ownarrivalSelect = document.getElementById("radioButtons-ownarrival");
flightSelect = document.getElementById("radioButtons-flighthotel");

if (newFormName == "flattrip") {
flattripForm.style.display = 'block';
ownarrivalForm.style.display = 'none';
flightForm.style.display = 'none';
flattripSelect.checked = true;
ownarrivalSelect.checked = false;
flightSelect.checked = false;
}
if (newFormName == "ownarrival") {
flattripForm.style.display = 'none';
ownarrivalForm.style.display = 'block';
flightForm.style.display = 'none';
flattripSelect.checked = false;
ownarrivalSelect.checked = true;
flightSelect.checked = false;
}
if (newFormName == "flight") {
flattripForm.style.display = 'none';
ownarrivalForm.style.display = 'none';
flightForm.style.display = 'block';
flattripSelect.checked = false;
ownarrivalSelect.checked = false;
flightSelect.checked = true;
}
}
</script>
