<!-- 


function chkFormular()
{

				if(document.magicmail_form.name.value == "")  { 

				alert("Bitte geben Sie Ihren Namen an."); 

				document.magicmail_form.name.focus(); 

				return false; 

				} 
 
				var mm_email_check = document.magicmail_form.email.value.match(/^[a-z\d][\w\d_\.-]+@([a-z\d][a-z\d\-]+[a-z\d]\.)+\w{2,6}$/i); 

				if(!mm_email_check && document.magicmail_form.email.value !="")  { 

				alert("Bitte geben Sie eine gültige eMail-Adresse an."); 

				document.magicmail_form.email.focus(); 

				return false; 

				} 
 
				if(document.magicmail_form.vorname.value == "")  { 

				alert("Bitte geben Sie Ihren Vornamen an."); 

				document.magicmail_form.vorname.focus(); 

				return false; 

				} 
 
				if(document.magicmail_form.telefon.value == "")  { 

				alert("Bitte geben Sie Ihre Telefonnummer an."); 

				document.magicmail_form.telefon.focus(); 

				return false; 

				} 
 
				if(document.magicmail_form.email.value == "")  { 

				alert("Bitte geben Sie eine E-Mailadresse an."); 

				document.magicmail_form.email.focus(); 

				return false; 

				} 
 
}
// -->
</script>
